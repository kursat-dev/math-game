{
    "version": 2,
    "builds": [
        {
            "src": "admin-panel/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "admin-panel/dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/game-demo/(.*)",
            "dest": "/game-demo/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/admin-panel/$1"
        }
    ]
}